{
    "name": "joomla/database",
    "type": "joomla-package",
    "description": "Joomla Database Package",
    "keywords": ["joomla", "framework", "database"],
    "homepage": "https://github.com/joomla-framework/database",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "phpunit/dbunit": "~1.2",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Database\\": "src/",
            "Joomla\\Database\\Tests\\": "Tests/"
        }
    }
}
