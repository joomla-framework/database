{
    "name": "joomla/oauth1",
    "type": "joomla-package",
    "description": "Joomla OAuth1 Package",
    "keywords": ["joomla", "framework", "oauth1"],
    "homepage": "https://github.com/joomla-framework/oauth1",
    "license": "GPL-2.0+",
    "require": {
        "php": "^5.3.10|~7.0",
        "joomla/application": "~1.0|~2.0",
        "joomla/http": "^1.2.2|~2.0",
        "joomla/input": "~1.2|~2.0",
        "joomla/registry": "^1.4.5|~2.0",
        "joomla/session": "~1.0|~2.0",
        "paragonie/random_compat": "~1.0|~2.0"
    },
    "require-dev": {
        "joomla/coding-standards": "~2.0@alpha",
        "joomla/event": "~1.0",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "^4.8.35|^5.4.3|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth1\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\OAuth1\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
