{
    "name": "joomla/uri",
    "type": "joomla-package",
    "description": "Joomla Uri Package",
    "keywords": ["joomla", "framework", "uri"],
    "homepage": "https://github.com/joomla-framework/uri",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10|>=7.0"
    },
    "require-dev": {
	    "joomla/test": "~1.0",
        "phpunit/phpunit": "~4.8|~5.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Uri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Uri\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
