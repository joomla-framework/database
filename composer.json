{
    "name": "joomla/archive",
    "type": "joomla-package",
    "description": "Joomla Archive Package",
    "keywords": ["joomla", "framework", "archive"],
    "homepage": "https://github.com/joomla-framework/archive",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/filesystem": "~1.0"
    },
    "require-dev": {
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Archive\\": "src/",
            "Joomla\\Archive\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
